 {
	"comments": {
		"lineComment": ";",
		"blockComment": [ "#|", "|#" ]
	},
	"brackets": [
		["(", ")"],
		["[", "]"]
	],
	"noautoClosingPairs": [
		{ "open": "[", "close": "]", "notIn": ["string"] },
		{ "open": "(", "close": ")", "notIn": ["string"] },
		{ "open": "\"", "close": "\"", "notIn": ["string", "comment"] }
	]
	//,"wordPattern": "[^() ]+"
	//,"editor.wordSeparators": "() "
}