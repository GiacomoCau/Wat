;; Utility used by `%makeStandardClass' and `defClass' tests.

(def\ classes-test (c d p q)
  (assert (type? c StandardClass))
  (assert (type? c Class))
  (assert (type? c Object))
  (assert (eq? (className c) 'Point2d))
  (assert (subClass? c StandardObject))
  (assert (subClass? c Object))

  (assert (type? p c))
  (assert (type? p StandardObject))
  (assert (type? p Object))
  (assert (eq? (getSlot p 'x) 1))
  (assert (eq? (getSlot p 'y) 2))

  (assert (type? d StandardClass))
  (assert (type? d Class))
  (assert (type? d Object))
  (assert (eq? (className d) 'Point3d))
  (assert (subClass? d c))
  (assert (subClass? d StandardObject))
  (assert (subClass? d Object))

  (assert (type? q d))
  (assert (type? q c))
  (assert (type? q StandardObject))
  (assert (type? q Object))
  (assert (eq? (getSlot q 'x) 1))
  (assert (eq? (getSlot q 'y) 2))
  (assert (eq? (getSlot q 'z) 3)))

