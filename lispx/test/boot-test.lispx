(defsuite makeTypeError

  (deftest makeTypeError.1
    (let ((e (makeTypeError "foo" 'Number)))
      (assert (type? e TypeError))
      (assert (type? e Error))
      (assert (type? e Condition))
      (assert (eq? "foo" (getSlot e 'datum)))
      (assert (eq? 'Number (getSlot e 'expectedType))))
    #inert))

(defsuite simpleError

  (deftest makeSimpleError.1
    (let ((e (makeSimpleError "foo")))
      (assert (type? e SimpleError))
      (assert (type? e Error))
      (assert (type? e Condition))
      (assert (eq? "foo" (getSlot e 'message))))
    #inert)

  (deftest simpleError.1
    (signalsError
     (simpleError)
     MatchError))

  (deftest simpleError.2
    (signalsError
     (simpleError "foo")
     SimpleError :message "foo")))

(defsuite makeRestartError

  (deftest makeRestartError.1
    (let ((e (makeRestartError 'continue)))
      (assert (type? e RestartError))
      (assert (type? e Error))
      (assert (type? e Condition))
      (assert (eq? 'continue (getSlot e 'restartName))))
    #inert))

(defsuite vau

  (deftest |A fexpr without body expression results in #VOID.|
    ((vau #ignore #ignore))
    #inert)

  (deftest |A fexpr returns its operands unevaluated.|
    ((vau ops #ignore ops) x y z)
    '(x y z))

  (deftest |A fexpr can have multiple body expressions.|
    (begin
      (def env (theEnv))
      (assert (eq? ((vau ops #ignore (set env var 1) ops) x y z)
                 '(x y z)))
      (assert (eq? (eval 'var env) 1)))
    #inert)

  (deftest |The environment parameter receives the current environment.|
    (begin
      (def env (theEnv))
      (== ((vau #ignore e e)) env)))

  (deftest |Fexprs are instances of FEXPR, OPERATOR, OBJECT.|
    (let ((fexpr (vau #ignore #ignore 12)))
      (type? fexpr Fexpr)
      (type? fexpr Operator)
      (type? fexpr Object)
      (fexpr))
    12)

  (deftest |Arity check 1.|
    (signalsError
     (vau)
     MatchError))

  (deftest |Arity check 2.|
    (signalsError
     (vau x)
     MatchError)))

(defsuite quote

  (deftest |Quoted symbols are pointer identical.|
    (and
     (== 'foo 'foo)
     (== 'foo 'foo)
     (== 'foo (functionSymbol 'foo))
     (== 'Foo 'Foo)
     (== 'Foo (classSymbol 'Foo))))

  (deftest |Ordinary symbols can be quoted.|
    (== 'foo (variableSymbol 'foo)))

  (deftest |Function symbols can be quoted.|
    (== 'foo (functionSymbol 'foo)))

  (deftest |Class symbols can be quoted.|
    (== 'Foo (classSymbol 'Foo)))

  (deftest |Lists can be quoted.|
    (eq? '(1 2 3) (list 1 2 3)))

  (deftest |Numbers can be quoted.|
    (eq? '1 1))

  (deftest |Strings can be quoted.|
    (eq? '"foo" "foo"))

  (deftest |QUOTE requires at least one argument.|
    (signalsError
     (quote)
     MatchError))

  (deftest |QUOTE requires at most one argument.|
    (signalsError
     (quote 1 2 3)
     MatchError)))

